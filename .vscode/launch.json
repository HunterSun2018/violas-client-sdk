    "version": "0.2.0",
    "configurations": [              
        {
            "type": "lldb",
            "request": "launch",
            "name": "violas-oracle",
            "program": "${workspaceFolder}/rust/oracle-service/target/debug/violas-oracle",
            "args": [
                "update",
                "-u",
                "http://localhost:8080",
                "-m",
                "/tmp/libra/mint.key",
                "-w",
                "0:170cc0c698d8b673a3b5fe884777f1144712ba89bffbde58d43483b9d29da154",
                "-c",
                "4",
                "-n",
                "${workspaceFolder}/rust/oracle-service/target/debug/oracle.m"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch violas_client",
            "program": "${workspaceFolder}/build/debug/bin/violas_client",
            "args": [
                "http://localhost:46441",
                "/tmp/libra/mint.key",
                "build/debug/client.mne",
                "0:35bc1b1a6ef4fce945aecdd21d637f582f5151f0830a9440e67e68287f6a2bda",
                "4",
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch violas_vls",
            "program": "${workspaceFolder}/build/debug/bin/violas_vls",
            "args": [
                "http://localhost:40955",
                "/tmp/libra/mint.key",
                "waypoint",
                "0:63d1cab20e52e3e6d7babbe07c8e195dfec7c156ffe30e9c4679fd5c38a3e59e",
                "4"
            ],            
            "cwd": "${workspaceFolder}/build/debug"
        },
        {
            "name": "Java Launch jni",
            "type": "cppdbg",
            "request": "launch",
            "program": " /usr/bin/java",
            "args": [
                "--module-path",
                "/usr/share/openjfx/lib/",
                "--add-modules=javafx.base",
                "-cp",
                "../bin/violas.jar",
                "Demo.java",
                "localhost",
                "33809",
                "/tmp/bc471173615e33d98a71c41f8a132c89/mint.key",
                "mnemonic"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/Java/test",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch transfer",
            "program": "${workspaceFolder}/build/debug/bin/transfer",
            "args": [
                "18.220.66.235",
                "50001",
                "../../cppSdk/testnet/chairman",
                "/home/hunter/Downloads/mint_test.key"
            ],
            "cwd": "${workspaceFolder}/build/debug"
        },
        {
            "name": "Python: violas.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cppSdk/test/violas.py",
            "console": "integratedTerminal"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "(lldb) Launch",
            "program": "${workspaceFolder}/cppSdk/test/test",
            "args": [],
            "cwd": "${workspaceFolder}/cppSdk/test"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cppSdk/test/test",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/cppSdk/test",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
