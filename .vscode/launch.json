    "version": "0.2.0",
    "configurations": [               

        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch testnet",
            "program": "${workspaceFolder}/build/debug/bin/testnet",
            "args": ["localhost", "36817", "governor", "/tmp/8a03b1bf30f48a37520b67d620ff6a14/mint.key"],
            "cwd": "${workspaceFolder}/build/debug"
        },

        {
            "name": "Java Launch jni",
            "type": "cppdbg",
            "request": "launch",
            "program": " /usr/bin/java",
            "args": ["--module-path", "/usr/share/openjfx/lib/", "--add-modules=javafx.base", "-cp", "../bin/violas.jar", "Demo.java"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/Java/test",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch transfer",
            "program": "${workspaceFolder}/build/debug/bin/transfer",
            "args": [
                "18.220.66.235",
                "50001",
                "../../cppSdk/testnet/chairman",
                "/home/hunter/Downloads/mint_test.key"
            ],
